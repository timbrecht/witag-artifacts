echo "#Filename               Expected Rates Low              Expected Rates High     Actual Msg Rates Low            Actual Msg Rates High" > message-rates.txt
grep Delay *.out | awk '{printf "%.1f\n",$3/1000}' > tmp1
fgrep "% conf interval: " expected.txt  | awk -F ",| "  '{print $4" "$6}' | awk -F "[" '{print $1" "$2}'  | awk -F "]" '{print $1 "   " $2}' > tmp2
fgrep "% conf interval: " messages.txt  | awk -F ",| "  '{print $4" "$6}' | awk -F "[" '{print $1" "$2}'  | awk -F "]" '{print $1 "   " $2}' > tmp3
paste tmp1 tmp2 tmp3 >> message-rates.txt
cat message-rates.txt
