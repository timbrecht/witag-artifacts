#!/bin/csh

foreach i ($*)
  echo $i
  echo -n "47: "
  fgrep -c "len =  47" $i
  echo -n "48: "
  fgrep -c "len =  48" $i
  echo -n "49: "
  fgrep -c "len =  49" $i
  echo -n "50: "
  fgrep -c "len =  50" $i
  # echo -n "51: "
  # fgrep -c "len =  51" $i
  echo -n "len = 50 (7 zeros 41 ones) [10000111111111111111111111111111111111111111110001] : "
  fgrep -c "[10000111111111111111111111111111111111111111110001]" $i
  echo -n "len = 50 (7 zeros 41 ones) [10001111111111111111111111111111111111111111100001] : "
  fgrep -c "[10001111111111111111111111111111111111111111100001]" $i
  echo -n "len = 50 (8 zeros 40 ones) [10000111111111111111111111111111111111111111100001] : "
  fgrep -c "[10000111111111111111111111111111111111111111100001]" $i
  echo -n "len = 50 (8 zeros 40 ones) [10000011111111111111111111111111111111111111110001] : "
  fgrep -c "[10000011111111111111111111111111111111111111110001]" $i
  echo -n "len = 50 (8 zeros 40 ones) [10001111111111111111111111111111111111111111000001] : "
  fgrep -c "[10001111111111111111111111111111111111111111000001]" $i
  echo "-----------------"
end
